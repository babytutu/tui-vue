html, body {
  height: 100%;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
}

a {
  text-decoration: none;
  color: #333
}

.content{
  font-size: 14Px
}
.content h1, .content h2, .content h3 {
  color: #444;
  font-size: 1.2em;
  margin: 1.5em 0 1.2em;
}
.content h1 {
  font-size: 2em;
}
.content h3 {
  font-size: 1em;
}
.content p {
  line-height: 1.6em;
}
.content ul {
  list-style-type: disc;
  margin: 10px 0;
}
.content ul li {
  margin-left: 30px;
  line-height: 1.6em;
}
.content hr {
  border: 0;
  border-bottom: 1px solid #f0f0f0;
}
.content pre {
  background-color: #f8f8f8;
  padding: 10px;
  margin: 1.2em 0;
  overflow: auto;
  line-height: 1.6em;
}
.content p > code {
  background-color: #eee;
  padding: 2px;
  color: #26a2ff;
}
.content blockquote {
  margin-bottom: 20px;
}
.content a {
  color: #4078c0;
  text-decoration: none;
}

.content table th:nth-child(3),
.content table th:nth-child(5) {
  width: 65px;
}
.content table th:nth-child(4) {
  width: 110px;
}
@media screen and (max-width: 768px) {
  .content .table-container {
    overflow-x: scroll;
  }
  .content .table {
    width: auto;
    min-width: 100%;
    table-layout: auto;
  }
  .content .table-container .table th {
    width: auto;
    min-width: 100px;
  }
}

/* Remove table cell spacing and padding */
table {
  border-collapse: collapse;
  border-spacing: 0;
}
td,
th {
  padding: 0;
}

.table {
  width: 100%;
  max-width: 100%;
  margin-bottom: 16px;
  margin-bottom: 1em;
  table-layout: fixed;
  background-color: #fff;
}
.table td, .table th {
  padding: 8px;
  vertical-align: top;
  border: 1px solid #eee;
  text-align: left;
  color: #656b79;
}
.table th {
  font-weight: 400;
  padding: 8px;
  background-color: #f4f5f7;
}
.table .table-center, .table .table-center th, .table .table-center td {
  text-align: center;
}

.btn-demo{
  width 100%
  padding 20px
  button{
    display block
    margin-bottom 20px
  }
}